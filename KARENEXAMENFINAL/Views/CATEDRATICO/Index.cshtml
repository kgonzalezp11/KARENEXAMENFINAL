@model IEnumerable<FINAL.CATEDRATICO>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Listado de Catedraticos</h2>

<p>
    @Html.ActionLink("Crear Nuevo Catedratico", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre_Catedratico)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellido_Catedratico)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefono_Catedratico)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estado_Catedratico)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre_Catedratico)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellido_Catedratico)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefono_Catedratico)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estado_Catedratico)
        </td>
        <td>
            @Html.ActionLink("Modificar", "Edit", new { id=item.ID_Catedratico }) |
            @Html.ActionLink("Detalle", "Details", new { id=item.ID_Catedratico }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.ID_Catedratico })
        </td>
    </tr>
}

</table>
